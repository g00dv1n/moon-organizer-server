    "flow:typed": "./node_modules/.bin/flow-typed install --overwrite",
    "test": "./node_modules/.bin/tape -r babel-register ./tests/**/*.js",
    "db:seed": "node_modules/.bin/babel-node ./src/seed.js"
  },
